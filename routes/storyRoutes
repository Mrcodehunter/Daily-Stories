//const stories = require("../controllers/storyController.js");
const storyServiceDaoObject = require("../dao/serviceDao").storyServiceDaoObject;
var router = require("express").Router();
module.exports = app => {

  router.post("/", storyServiceDaoObject.createStory);
  router.get("/", storyServiceDaoObject.getAllStory);
  router.get("/:id", storyServiceDaoObject.getStory);
  router.patch("/:id", storyServiceDaoObject.updateStory);
  router.delete("/:id", storyServiceDaoObject.deleteStory);
  app.use('/api/v1/stories', router);

 };


