//const stories = require("../controllers/storyController.js");
const storyServiceDaoObject = require("../dao/serviceDao").storyServiceDaoObject;
const verifyToken = require("../middleware/verifyToken");
var router = require("express").Router();

router
.route('/api/v1/stories')
.post(verifyToken, storyServiceDaoObject.createStory)
.get( storyServiceDaoObject.getAllStory);

router
.route('/api/v1/stories/:id')
.get( storyServiceDaoObject.getStory)
.patch(verifyToken, storyServiceDaoObject.updateStory)
.delete(verifyToken, storyServiceDaoObject.deleteStory);

module.exports = router


